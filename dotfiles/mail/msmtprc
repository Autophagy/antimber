account autophagy
host mail.gandi.net
port 465
protocol smtp
auth on
user mail@autophagy.io
passwordeval gpg --quiet --for-your-eyes-only --no-tty --decrypt ~/.msmtp-autophagy.gpg
from mail@autophagy.io
tls on
tls_starttls off

